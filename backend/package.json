{
  "name": "footballhome-backend",
  "version": "1.0.0",
  "description": "Football Home Team Management Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test-geocoding": "node -e \"require('dotenv').config({path:'../.env'});const G=require('./services/GeocodingService');new G().geocode('1600 Amphitheatre Parkway, Mountain View, CA').then(console.log).catch(console.error)\"",
    "test-venue": "node -e \"require('dotenv').config({path:'../.env'});const V=require('./services/VenueService');new V().getStats().then(console.log).then(()=>process.exit(0)).catch(console.error)\"",
    "health-check": "curl -s http://localhost:3001/health | jq"
  },
  "keywords": [
    "football",
    "team",
    "management",
    "api",
    "google-maps"
  ],
  "author": "Football Home",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^7.0.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  }
}
